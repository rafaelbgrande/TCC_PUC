      <name>Create Schema [PUC] and Tables</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>drop schema if exists puc cascade;

create schema puc;


create table puc.tb_perfil
(
     id                        serial          not null
   , nome                      varchar(100)    not null
   , fl_ativo                  boolean         not null constraint df_tb_perfil_fl_ativo    default(True)
   , dt_inclusao               timestamp       not null constraint df_tb_perfil_dt_inclusao default(current_timestamp)
   , dt_alteracao              timestamp       null
   , dt_exclusao               timestamp       null
   , constraint pk_tb_perfil   primary key(id)
);



create table puc.tb_usuario
(
     id                        serial          not null
   , nome                      varchar(100)    not null
   , email                     varchar(100)    not null
   , id_perfil                 int             not null
   , fl_ativo                  boolean         not null constraint df_tb_usuario_fl_ativo    default(True)
   , dt_inclusao               timestamp       not null constraint df_tb_usuario_dt_inclusao default(current_timestamp)
   , id_usuario_inclusao       int             not null
   , dt_alteracao              timestamp       null
   , id_usuario_alteracao      int             null
   , dt_exclusao               timestamp       null
   , id_usuario_exclusao       int             null
   , constraint pk_tb_usuario                          primary key(id)
   , constraint uk_tb_usuario                          unique(email)
   , constraint fk_tb_usuario_id_perfil                foreign key(id_perfil)              references puc.tb_perfil(id)
   , constraint fk_tb_usuario_id_usuario_inclusao      foreign key(id_usuario_inclusao)    references puc.tb_usuario(id)
   , constraint fk_tb_usuario_id_usuario_alteracao     foreign key(id_usuario_alteracao)   references puc.tb_usuario(id)
   , constraint fk_tb_usuario_id_usuario_exclusao      foreign key(id_usuario_exclusao)    references puc.tb_usuario(id)
);



create table puc.tb_gravadora
(
     id                        serial          not null
   , nome                      varchar(100)    not null
   , fl_ativo                  boolean         not null constraint df_tb_gravadora_fl_ativo    default(True)
   , dt_inclusao               timestamp       not null constraint df_tb_gravadora_dt_inclusao default(current_timestamp)
   , id_usuario_inclusao       int             not null
   , dt_alteracao              timestamp       null
   , id_usuario_alteracao      int             null
   , dt_exclusao               timestamp       null
   , id_usuario_exclusao       int             null
   , constraint pk_tb_gravadora                           primary key(id)
   , constraint uk_tb_gravadora                           unique(nome)
   , constraint fk_tb_gravadora_id_usuario_inclusao       foreign key(id_usuario_inclusao)    references puc.tb_usuario(id)
   , constraint fk_tb_gravadora_id_usuario_alteracao      foreign key(id_usuario_alteracao)   references puc.tb_usuario(id)
   , constraint fk_tb_gravadora_id_usuario_exclusao       foreign key(id_usuario_exclusao)    references puc.tb_usuario(id)
);



create table puc.tb_genero
(
     id                        serial          not null
   , nome                      varchar(100)    not null
   , id_genero_pai             int             null
   , fl_ativo                  boolean         not null constraint df_tb_genero_fl_ativo    default(True)
   , dt_inclusao               timestamp       not null constraint df_tb_genero_dt_inclusao default(current_timestamp)
   , id_usuario_inclusao       int             not null
   , dt_alteracao              timestamp       null
   , id_usuario_alteracao      int             null
   , dt_exclusao               timestamp       null
   , id_usuario_exclusao       int             null
   , constraint pk_tb_genero                           primary key(id)
   , constraint uk_tb_genero                           unique(nome, id_genero_pai)
   , constraint fk_tb_genero_id_genero_pai             foreign key(id_genero_pai)          references puc.tb_genero(id)
   , constraint fk_tb_genero_id_usuario_inclusao       foreign key(id_usuario_inclusao)    references puc.tb_usuario(id)
   , constraint fk_tb_genero_id_usuario_alteracao      foreign key(id_usuario_alteracao)   references puc.tb_usuario(id)
   , constraint fk_tb_genero_id_usuario_exclusao       foreign key(id_usuario_exclusao)    references puc.tb_usuario(id)
);



create table puc.tb_musica
(
     id                        serial          not null
   , id_gravadora              int             not null
   , nome                      varchar(150)    not null
   , interpretes               varchar(4000)   not null
   , ano                       smallint        not null
   , fl_ativo                  boolean         not null constraint df_tb_musica_fl_ativo    default(True)
   , dt_inclusao               timestamp       not null constraint df_tb_musica_dt_inclusao default(current_timestamp)
   , id_usuario_inclusao       int             not null
   , dt_alteracao              timestamp       null
   , id_usuario_alteracao      int             null
   , dt_exclusao               timestamp       null
   , id_usuario_exclusao       int             null
   , constraint pk_tb_musica                           primary key(id)
   , constraint uk_tb_musica                           unique(id_gravadora, nome, interpretes)
   , constraint fk_tb_musica_id_gravadora              foreign key(id_gravadora)           references puc.tb_gravadora(id)
   , constraint fk_tb_musica_id_usuario_inclusao       foreign key(id_usuario_inclusao)    references puc.tb_usuario(id)
   , constraint fk_tb_musica_id_usuario_alteracao      foreign key(id_usuario_alteracao)   references puc.tb_usuario(id)
   , constraint fk_tb_musica_id_usuario_exclusao       foreign key(id_usuario_exclusao)    references puc.tb_usuario(id)
);



create table puc.tb_musica_genero
(
     id                        serial          not null
   , id_musica                 int             not null
   , id_genero                 int             not null
   , constraint pk_tb_musica_genero                 primary key(id)
   , constraint uk_tb_musica_genero                 unique(id_musica, id_genero)
   , constraint fk_tb_musica_genero_id_musica       foreign key(id_musica)    references puc.tb_musica(id)
   , constraint fk_tb_musica_genero_id_genero       foreign key(id_genero)    references puc.tb_genero(id)
);



create table puc.tb_log_play
(
     id                        bigserial       not null
   , id_usuario                int             not null
   , id_musica                 int             not null
   , cod_tipo                  smallint        not null
   , tempo_antes               decimal(8, 3)   not null
   , tempo_atual               decimal(8, 3)   not null
   , tempo_total               decimal(8, 3)   not null
   , dt_play                   timestamp       not null constraint df_tb_log_play_dt_play default(current_timestamp)
   , constraint pk_tb_log_play   primary key(id)
);



create table puc.tb_log_download
(
     id                        bigserial       not null
   , id_usuario                int             not null
   , id_musica                 int             not null
   , dt_download               timestamp       not null constraint df_tb_log_download_dt_download default(current_timestamp)
   , constraint pk_tb_log_download   primary key(id)
);

</sql>
